      $(document).ready(function() {
      
      $.ajax(
      {url : 'http://localhost:8888/', dataType : 'json', data : {
      name : "Marcus" },
      success : function(data)  {
      
      $("#content").text(data.name);
      
      },
      error : function(req,data,err) {
      console.log(err);
      }
      
      }); 
      });   
